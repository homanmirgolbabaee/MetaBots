{
    "openapi": "3.0.0",
    "info": {
      "title": "GitHub API",
      "version": "1.0.0"
    },
    "paths": {
      "/repos/{owner}/{repo}/clone": {
        "post": {
          "summary": "Clone a repository",
          "parameters": [
            {
              "name": "owner",
              "in": "path",
              "required": true,
              "schema": {
                "type": "string"
              }
            },
            {
              "name": "repo",
              "in": "path",
              "required": true,
              "schema": {
                "type": "string"
              }
            }
          ],
          "responses": {
            "200": {
              "description": "Repository cloned successfully"
            }
          }
        }
      }
    }
  }
  